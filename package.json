{
  "name": "scribe",
  "version": "0.1.0",
  "description": "Javascript transpiler for the Scribe language",
  "main": "bin/scribe.js",
  "scripts": {
    "start": "node bin/scribe.js",
    "postinstall": "npm run antlr",
    "antlr": "antlr4 -Dlanguage=JavaScript -long-messages grammar/*.g4"
  },
  "keywords": [
    "scribe",
    "language",
    "transpiler"
  ],
  "author": "Brandon Smith",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com:brundonsmith/scribe.git"
  },
  "devDependencies": {
    "antlr4-cli": "^4.5.3",
    "antlr4": "^4.5.3"
  }
}
